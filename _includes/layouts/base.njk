<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>
    <header>
      <h1 class="home">
        <a href="{{ '/' | url }}">
          <div class="sun">
            <img src="{{ '/img/sindre.jpeg' | url }}" alt="" />
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
 version="1.0" viewbox="0 0 100 100" width="300" height="300">
              <style>
                .sun-path {
                  fill: transparent;
                  stroke: #F4BE8B;
                  stroke-width: 3;
                }
              </style>
              <path class="sun-path" d="M50 0 l0 10 M90 10 l-8 8 M100 50 l-10 0 M90 90 l-8 -8 M50 100 l0 -10 M10 90 l8 -8 M0 50 l10 0 M10 10 l8 8"/>
            </svg>
          </div>
            <span>Sindre</span>
          {# <img class="logo" alt="Home" src="{{ '/img/signature.svg' | url }}" /> #}
        </a>
      </h1>
      <ul class="nav">
        {%- for nav in collections.nav | reverse -%}
          <li class="nav-item{% if nav.url == page.url %} nav-item-active{% endif %}">
            <a href="{{ nav.url | url }}">{{ nav.data.navtitle }}</a>
          </li>
        {%- endfor -%}
      </ul>
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

    <footer></footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>